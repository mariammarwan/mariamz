#include<stdlib.h>
#include<stdio.h>

int main()
{
	char str[NO_OF_CHARS];	
	printf(“\nEnter the string : “);
	scanf(“%s”,&str);
  
  int index = firstNonRepeating(str);
    if (index == -1)
        printf("Either all characters are repeating or "
               "string is empty");
    else
        printf("First non-repeating character is %c",
               str[index]);
    return 0;
}
